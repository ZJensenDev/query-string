{
    "name": "chasegiunta/query-string",
    "description": "Work with query strings",
    "type": "craft-plugin",
    "version": "1.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "query string"
    ],
    "support": {
        "docs": "https://github.com/chasegiunta/query-string/blob/master/README.md",
        "issues": "https://github.com/chasegiunta/query-string/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Chase Giunta",
            "homepage": "https://chasegiunta.com"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0-alpha"
    },
    "autoload": {
        "psr-4": {
          "chasegiunta\\querystring\\": "src/"
        }
    },
    "extra": {
        "name": "Query String",
        "handle": "query-string",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/chasegiunta/query-string/master/CHANGELOG.md",
        "components": {
            "queryStringService": "chasegiunta\\querystring\\services\\QueryStringService"
        },
        "class": "chasegiunta\\querystring\\QueryString"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
}
